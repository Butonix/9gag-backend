  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamtalia/9gag-backend.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/*.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
